{
  "name": "image-metadata-inspector",
  "displayName": "Image Metadata Inspector", 
  "description": "Inspect and display metadata of JPG, PNG, and WebP images. Perfect for ComfyUI workflows.",
  "repository": {
    "type": "git",
    "url": "https://github.com/Gerkinfeltser/image-metadata-display.git"
  },
  "version": "0.1.0",
  "publisher": "Gerkinfeltser",
  "icon": "images/image_metadata_viewer_icon.png",
  "license": "MIT",
  "keywords": [
    "metadata",
    "exif", 
    "image",
    "comfyui",
    "workflow",
    "png",
    "jpg",
    "webp",
    "jpeg"
  ],
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.inspectImageMetadata",
        "title": "Inspect Image Metadata"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "resourceExtname == .jpg || resourceExtname == .png || resourceExtname == .webp || resourceExtname == .jpeg",
          "command": "extension.inspectImageMetadata",
          "group": "navigation"
        }
      ],
      "commandPalette": [
        {
          "command": "extension.inspectImageMetadata"
        }
      ],
      "editor/title/context": [
        {
          "when": "resourceExtname == .jpg || resourceExtname == .png || resourceExtname == .webp || resourceExtname == .jpeg",
          "command": "extension.inspectImageMetadata",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "test": "node ./node_modules/vscode-test/out/runTest.js",
    "build": "npm run compile",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "^18.0.0",
    "@types/vscode": "^1.87.2",
    "@vscode/test-electron": "^2.3.9",
    "@vscode/vsce": "^2.29.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "exiftool-vendored": "^24.6.0"
  }
}